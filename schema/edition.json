{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Edition",
  "description": "Metadata about an edition",
  "type": "object",
  "properties": {
    "edition": {
      "type": "string",
      "format": "date-time"
    },
    "description": {
      "type": "string",
      "maxLength": 2048
    },
    "startTime": {
      "type": "string",
      "format": "date-time"
    },
    "endTime": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false,
  "required": [
    "edition"
  ]
}
