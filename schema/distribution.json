{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Distribution",
  "description": "Metadata about a distribution",
  "type": "object",
  "properties": {
    "distribution_type": {
      "type": "string",
      "enum": ["file", "api"]
    },
    "content_type": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "filename": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "filenames": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255
      }
    },
    "api_url": {
      "type": "string",
      "maxLength": 2048
    },
    "license": {
      "description": "A legal document under which the resource is made available.",
      "type": "string"
    }
  },
  "additionalProperties": false
}
